DISCORD_TOKEN=your_discord_bot_token_here

# OpenAI API Configuration
OPENAI_API_KEY=your_openai_api_key_here
# Model options: gpt-4o, gpt-4o-mini, gpt-4-turbo, gpt-3.5-turbo
OPENAI_MODEL=gpt-4o-mini

# Neon Database Configuration
# Get your connection string from https://neon.tech
DATABASE_URL=postgresql://user:password@host.neon.tech/dbname?sslmode=require

# Dashboard Configuration
DASHBOARD_PORT=3000

# Channel monitoring options:
# - Set to ALL to monitor all channels: CHANNEL_IDS=ALL
# - Set specific channel IDs: CHANNEL_IDS=1176467419938701375,1178738361276563546
# - Leave empty to monitor all: CHANNEL_IDS=
CHANNEL_IDS=ALL
